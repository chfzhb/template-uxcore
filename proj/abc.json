{
    "name": "<%- name %>",
    "builder": "@ali/builder-alinw",
    "options": {
        "type": "uxcore",
        "template": "<%- template %>",
        "vars": {
            "locale": "zh-cn",
            "__LOCAL__": true
        },
        "buildvars": {
            "locale": [ "zh-cn", "en" ]
        },
        "pages": true<% if (library) { %>,
        "libraries": {
            "Uxcore": {
                "output": "uxcore.js",
                "mappings": {
                    "Button": "uxcore-button@~0.4.0"
                }
            }
        }<% } %>
    }
}
