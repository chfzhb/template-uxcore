{
  "name": "<%- name %>",
  "options": {
    "type": "uxcore",
    "template": "<%- template %>",
    "npm": "<%- npm %>",
    "vars": {
      <% if (i18n) { %>"locale": "zh-cn",
      <% } %>"__LOCAL__": true
    },
    "buildvars": {
      "__LOCAL__": [false]<% if (i18n) { %>,
      "locale": [ "zh-cn", "en" ]<% } %>
    },
    "pages": <%= !SPA %>,
    "externals": {
      "natty-fetch": "var nattyFetch",
      "react": "var React",
      "react-dom": "var ReactDOM",
      "react-router": "var ReactRouter",
      "Promise": "var Promise",
      "reflux": "var Reflux",
      "Uxcore": "var Uxcore",
      "lodash": "var _"
    },
    "mockapi": {
      "/mock/query/getSomeInfo.json": "mock/query/getSomeInfo.js"
    }<% if (libraries) { %>,
    "libraries": {
      "Uxcore": {
        "output": "uxcore.js",
        "mappings": {
          "Table": "uxcore-table@~1.9.0",
          "Menu": "uxcore-menu@~1.3.1"
        }
      }
    }<% } %>
  }
}
